local ImageButton = Instance.new("ImageButton")

ImageButton.Parent = game.StarterGui.ScreenGui
ImageButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ImageButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
ImageButton.BorderSizePixel = 0
ImageButton.Position = UDim2.new(0.243090004, 0, 0.0604938269, 0)
ImageButton.Size = UDim2.new(0, 678, 0, 220)
ImageButton.Image = "http://www.roblox.com/asset/?id=13979677676"

local function HSHTEJG_fake_script() -- ImageButton.LocalScript 
	local script = Instance.new('LocalScript', ImageButton)

	script.Parent.MouseButton1Click:Connect(function()
		while true do
	
		end
	end)
end
coroutine.wrap(HSHTEJG_fake_script)()
